{
    "name": "gymai-monorepo",
    "private": true,
    "workspaces": [
        "apps/*",
        "packages/*"
    ],
    "scripts": {
        "build:web": "turbo run app#build:web",
        "build:all": "turbo run app#build:web app#build:ios app#build:android app#build:macos",
        "codegen": "turbo run app#codegen",
        "analyze": "turbo run app#analyze",
        "test": "turbo run app#test backend#test",
        "dev:backend": "turbo run backend#dev"
    },
    "devDependencies": {
        "turbo": "^2.4.4"
    },
    "packageManager": "npm@10.9.2",
    "engines": {
        "node": ">=20"
    }
}